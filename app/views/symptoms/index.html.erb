<div class="container">
  <div class="notice"> <%= flash[:message] %> </div>

  <h1> Symptoms </h1>

    <ol>
      <% @symptoms.each do |symptom| %>
        <li><%= symptom.name %> | <%= link_to 'Delete Symptom', symptom, method: :delete, data:{confirm: "Are you sure you want to destroy this symptom?"} %>
        </li>
      <% end %>
    </ol>

</div>